{
  "name": "deploy-node-in-vercel",
  "version": 2,
  "public": true,
  "functions": {
    "api/index.js": {
      "runtime": "vercel-php@0.1.0",
      "env": {
        "VERCEL_RUNTIME_NAME": "php-8.0"
      }
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}
